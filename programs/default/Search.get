inputs { gene, page? }

GET https://www.biorxiv.org/search/:gene
[query]
page: $page

extract {
  papers: {
    items: => li.search-result -> @Paper({
      @link: a
      title: a
      authors: => .highwire-citation-author
      biorxiv: .highwire-cite-metadata-pages
      doi: //span[@class='doi_label']/following-sibling::text()
    })
    total: h1 -> `parseInt($)`
    pager: {
      next: @Search) li.pager-next a
      prev: @Search) li.pager-previous a
    }
  }
}
